{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "ProducerClient": {
      "BootstrapServers": "host.docker.internal:9092"
    },
    "AdminClient": {
      "BootstrapServers": "host.docker.internal:9092"
    },
    "Topics": {
      "User": "User"
    },
    "ProducerService": {
      "IntervalSeconds": 60,
      "DelaySeconds": 60
    }
  }
}
